{
    "name": "dev-extensions/kernel",
    "description": "dev-extensions kernel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Platform\\Kernel\\": "src/"
        }
    },
    "require": {
        "maatwebsite/excel": "^3.1",
        "google/apiclient": "^2.10",
        "asimlqt/php-google-spreadsheet-client": "3.0.2",
        "revolution/laravel-google-sheets": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Platform\\Kernel\\Providers\\KernelServiceProvider"
            ],
            "aliases": {}
        },
        "google/apiclient-services": [
            "Analytics",
            "Sheets",
            "Drive",
            "DriveActivity"
        ]
    },
    "repositories": [],
    "scripts": {
        "post-install-cmd": [
            "@php vendor/dev-extensions/kernel/bin/install-binaries.php",
            "@php artisan git:install-commit-hook"
        ],
        "post-update-cmd": [
            "@php vendor/dev-extensions/kernel/bin/install-binaries.php",
            "@php artisan git:install-commit-hook"
        ]
    }
}